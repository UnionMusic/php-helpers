{
    "name": "unionmuisc/helpers",
    "description": "Provides backwards compatibility for helpers in the Union Music.",
    "keywords": ["helpers"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Teakowa",
            "email": "git@teakowa.dev"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-zlib": "*",
        "tuupola/base62": "^2.0",
        "webpatser/laravel-uuid": "^3.0",
        "coduo/php-humanizer": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "illuminate/support": "^7.0.2",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/phpinsights": "^1.11",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
